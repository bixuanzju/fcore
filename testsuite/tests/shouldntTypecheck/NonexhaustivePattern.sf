data PolyList [A] = Nil
                | Cons A (PolyList[A])
                ;

let rec removeDuplicates [A] (x:PolyList[A]): PolyList[A] =
    case x of
        Nil -> x
      | Cons x (Cons y z) ->
            if x == y then Cons [A] x (removeDuplicates [A] z)
            else Cons [A] x (Cons [A] y (removeDuplicates [A] z))
in

removeDuplicates
